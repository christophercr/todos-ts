{
  "name": "todos-ts",
  "version": "0.1.0",
  "description": "Demo app for AngularJS training using Typescript and Webpack",
  "author": {
    "email": "christophercr@gmail.com",
    "name": "Christopher Cortes"
  },
  "keywords": [
    "web",
    "angularjs",
    "angular-material",
    "rxjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/christophercr/todos-ts"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-material": "1.1.1",
    "angular-messages": "1.5.8",
    "angular-mocks": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-touch": "1.5.8",
    "angular-translate": "2.12.1",
    "angular-translate-loader-static-files": "2.12.1",
    "angular-ui-router": "0.3.1",
    "app-root-path": "1.3.0",
    "compression-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "3.0.1",
    "core-js": "2.4.1",
    "css-loader": "0.25.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fakerest": "1.2.0",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.4.1",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-mocha-reporter": "2.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.0",
    "lodash": "4.16.4",
    "mkdirp": "0.5.1",
    "moment": "2.15.1",
    "ncp": "2.0.0",
    "nodemon": "1.10.2",
    "postcss-cssnext": "2.8.0",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "postcss-nesting": "2.3.1",
    "postcss-simple-extend": "1.0.0",
    "postcss-url": "5.1.2",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.8",
    "remap-istanbul": "0.5.1",
    "rimraf": "2.5.4",
    "rxjs": "5.0.0-rc.1",
    "sinon": "1.17.6",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "superstatic": "4.0.3",
    "todomvc-common": "1.0.0",
    "todomvc-app-css": "^1.0.1",
    "ts-helper": "0.0.1",
    "ts-loader": "0.8.2",
    "ts-node": "1.6.0",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typedoc": "0.4.5",
    "typescript": "2.0.3",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "0.14.1",
    "webpack-sha-hash": "1.0.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "typescript": "2.0.3"
  },
  "engines": {
    "node": ">=4.4.2",
    "npm": ">=3.7.3"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "node --max-old-space-size=4096 ./node_modules/webpack/bin/webpack --config ./config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "node --max-old-space-size=4096 ./node_modules/webpack/bin/webpack --config ./config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "ci": "npm run test",
    "clean": "rimraf reports dist .tmp",
    "clean:all": "npm run clean && rimraf typings && npm cache clean",
    "clean:dist": "rimraf dist",
    "clean:install": "npm install --no-optional",
    "clean:start": "npm start",
    "docs": "typedoc --options ./typedoc.json --exclude \"*.spec.ts\" ./src/",
    "karma": "karma",
    "lint": "tslint --config tslint.json --project ./tsconfig.json",
    "outdated": "npm outdated",
    "prebuild:dev": "npm run clean",
    "prebuild:prod": "npm run clean",
    "preclean:install": "npm run clean:all && rimraf node_modules",
    "preclean:start": "npm run clean",
    "preprofiling": "npm run clean && mkdirp reports/profiling",
    "preserver:prod": "npm run build:prod",
    "prestart": "npm run clean",
    "pretest": "npm run clean",
    "prewatch:dev": "npm run clean",
    "prewatch:prod": "npm run clean",
    "postinstall": "npm run typings-install",
    "profiling": "node --max-old-space-size=4096 ./node_modules/webpack/bin/webpack --config ./config/webpack.prod.js --progress -c --profile --json > reports/profiling/stats.json",
    "server": "npm run server:dev -- --inline",
    "preserver:dev": "npm run clean",
    "server:dev": "node --max-old-space-size=4096 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./config/webpack.dev.js --progress --profile --colors --watch --display-error-details --display-cached --open",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:dev:hmr:livereload": "npm run server:dev:hmr -- --inline",
    "server:prod": "npm run superstatic",
    "setup": "npm install --global typings rimraf && npm install --no-optional",
    "start": "npm run server",
    "start:hmr": "npm run server:dev:hmr",
    "start:hmr:livereload": "npm run server:dev:hmr:livereload",
    "start:dev": "npm run server",
    "start:prod": "npm run server:prod",
    "superstatic": "cd dist && superstatic --config superstatic.json --port 8080 --host 0.0.0.0",
    "test": "node --max-old-space-size=4096 ./node_modules/karma/bin/karma start",
    "test-subset": "npm run test -- --path--",
    "tsc": "tsc",
    "typings": "typings",
    "typings-install": "typings install",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:test-subset": "npm run watch:test -- --path--",
    "watch:testd": "npm run test -- --auto-watch --no-single-run --browsers=Chrome"
  }
}
